<!doctype html>
<html lang="en">
  <head>
    <title>{{ title }}</title>
    <meta charset="utf-8" />
    <meta name="Author" content="State of California" />
    <meta name="Description" content="State of California" />
    <meta name="Keywords" content="California, government" />
    {% include "../../src/components/head-js-css.njk" %}
  </head>
  <body class="primary">
    <header role="banner" id="header" class="global-header fixed">
      <div id="skip-to-content">
        <a href="#main-content">Skip to Main Content</a>
      </div>
      {% include "../../src/components/utility-header.njk" %} {% include
      "../modules/branding.html" %} {% include
      "../../src/components/mobile-controls.njk" %}
      <div class="navigation-search">
        <!-- navigation types -->
        {% if navigation -%} {%- include navigation -%} {%- else -%} {%- include
        "src/componentsnavigation.njk" -%} {%- endif %}
        <div
          id="head-search"
          class="search-container {{ - ' featured-search' if search === 'Featured' }}">
          {% include "../../src/components/search.njk" %}
        </div>
      </div>
    </header>
    {% block content %}
    <div id="main-content" class="main-content">
      <div class="container p-b-lg p-t-md">
        <main class="main-primary" id="main">
          {# Breadcrumbs (rendering data from /src/_data/breadcrumbs.json and
          /src/_data/helpers.js #} {% include
          "../../src/components/breadcrumbs.njk" %} {{ content | safe }}
        </main>
      </div>
    </div>
    {% endblock %} {% include "../../src/components/global-footer.njk" %}
  </body>
</html>
